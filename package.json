{
  "name": "simple-git-history",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./",
    "start:dev": "npm run clear && npm-run-all --parallel build:dev main:dev",
    "start:hot": "npm run clear && npm-run-all --parallel server:hot build:hot main:hot",
    "start:prod": "npm run pre:start:prod && npm run main:prod",
    "pre:start:prod": "npm run clear && npm run build:prod",
    "server:hot": "NODE_ENV=development BUILD_TYPE=hot node ./renderer",
    "build:dev": "NODE_ENV=development webpack --config ./renderer/webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --config ./renderer/webpack.config.js",
    "build:hot": "NODE_ENV=development BUILD_TYPE=hot webpack --config ./renderer/webpack.config.js",
    "clear": "rm -rf ./build",
    "main:dev": "NODE_ENV=development npm run main",
    "main:prod": "NODE_ENV=production npm run main",
    "main:hot": "NODE_ENV=development BUILD_TYPE=hot npm run main",
    "main": "electron ./index.js"
  },
  "repository": "git@github.com:nerjs/simple-git-history.git",
  "author": "nerjs <karifan.stap@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "body-parser": "^1.19.0",
    "debounce": "^1.2.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "electron": "6.1.4",
    "electron-devtools-installer": "^3.0.0",
    "express": "^4.17.1",
    "git-url-parse": "^11.1.2",
    "morgan": "^1.9.1",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "qs": "^6.9.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-icons-kit": "^1.3.1",
    "react-router-dom": "^5.1.2",
    "react-use": "^13.14.0",
    "styled-components": "^4.4.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.5",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2",
    "webpack-cli": "^3.3.10"
  }
}
